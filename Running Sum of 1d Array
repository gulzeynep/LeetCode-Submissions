# Given an array nums. We define a running sum of an array as runningSum[i] = sum(nums[0]â€¦nums[i]).
# Return the running sum of nums.

class Solution(object):
    def runningSum(self, nums):
        """
        :type nums: List[int]
        :rtype: List[int]
        """
     
        temp=0
        answer=[]
        for i in range(1,len(nums)+1):
            temp+=nums[i-1]
            answer.append(temp)
            
        return answer

        
           
